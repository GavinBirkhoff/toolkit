{
  "name": "utils",
  "version": "1.0.0",
  "description": "Store more front-end related tool Libraries",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm install && lerna bootstrap",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "build": "lerna run build",
    "eslint": "eslint --ext .js,.ts --fix ./packages/**/src",
    "tslint": "tslint ./packages/**/*.ts",
    "tslint-fix": "tslint ./packages/**/*.ts --fix",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "script:gen-index": "lerna run script:gen-index",
    "gi": "lerna run script:gen-index",
    "docs": "lerna run docs",
    "pb": "lerna publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "prettier .prettierrc.js --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GavinBirkhoff/util.git"
  },
  "keywords": [
    "util"
  ],
  "author": "Gavin Birkhoff <gavinbirkhoff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GavinBirkhoff/util/issues"
  },
  "homepage": "https://github.com/GavinBirkhoff/util#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "docdash": "^1.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-html-reporter": "^3.4.1",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.3.2"
  }
}
